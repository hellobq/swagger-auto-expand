{
  "manifest_version": 3,
  "name": "Swagger Auto Expand",
  "version": "1.0",
  "description": "Automatically expands all Swagger@2.x categories.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/swagger-ui.html"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
} 