{
  "manifest_version": 3,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/swagger-ui.html"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "name": "swagger-auto-expand",
  "version": "0.0.1",
  "description": "Chrome extension to automatically expand Swagger@2.x categories"
}
